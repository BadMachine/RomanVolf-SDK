{
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "name": "romanvolf-sdk",
  "description": "LOTR API SDK",
  "main": "lib/LotrApi.js",
  "types": "lib/LotrApi.d.ts",
  "version": "1.0.0",

  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "test": "jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BadMachine/RomanVolf-SDK.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BadMachine/RomanVolf-SDK/issues"
  },
  "homepage": "https://github.com/BadMachine/RomanVolf-SDK#readme",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^1.0.0",
    "ts-node": "^10.9.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  }
}
